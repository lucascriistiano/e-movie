channel exibicao:{0..14}
channel cadeira:{0..99}
channel compra, cadastro, cancelamento, troca
channel id:{1..}

COMPRA = exibicao?x -> cadeira?x -> ESCOLHA

CADASTRO = email -> senha ->  SKIP

CANCELAMENTO = exibicaoCancelada?x -> SKIP

TROCA = exibicaoCancelada?x -> exibicao?x -> cadeira?x -> SKIP

CLIENTE = ESCOLHA

ESCOLHA = compra?id -> COMPRA
          []
          cadastro?id -> CADASTRO
          []
          cancelamento?id -> CANCELAMENTO
          []
          troca?id -> TROCA
